<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="eagledata.core.menus.eagledatarunner"
               style="push">
               <visibleWhen
                  checkEnabled="false">
               <not>
                  <reference
                        definitionId="eagledata.core.nature.hasNature">
                  </reference>
               </not>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="eagledata.core.menus.eagledatarunner"
            name="Run EagleData Project">
            
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="eagledata.core.menus.runner.EagleDataRunnerHandler"
            commandId="eagledata.core.menus.eagledatarunner">
      </handler>
   </extension>
	<extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="eagledata.core.nature.hasNature">
         <adapt
               type="org.eclipse.core.resources.IProject">
            <test
                  property="org.eclipse.core.resources.projectNature"
                  value="eagledata.core.nature.eagledatanature">
            </test>
         </adapt>
      </definition>
   </extension>
</plugin>
